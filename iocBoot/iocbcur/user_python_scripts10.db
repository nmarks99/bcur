record(bi, "$(P)PythonScript:Running") {
    field(DTYP, "pydev")
    field(INP, "@pydev.iointr('RUNNING')")
    field(SCAN, "I/O Intr")
}

record(stringout, "$(P)PythonScript1") {
    field(OUT, "$(P)RunPythonScript1.VAL NPP")
    field(PINI,1)
}

record(stringout, "$(P)RunPythonScript1") {
    field(DESC, "Script name without .py extension")
    field(DTYP, "pydev")
    field(VAL, "")
    field(OUT, "@from VAL import main; main()")
}

